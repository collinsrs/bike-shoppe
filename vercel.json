{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/fonts/ibm-plex-sans-var.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/ibm-plex-sans-var-italic.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/linkedin",
      "destination": "https://linkedin.com/in/collinsrs"
    },
    {
      "source": "/github",
      "destination": "https://github.com/collinsrs"
    },
    {
      "source": "/resume",
      "destination": "https://rishicollins.com/resume"
    },
    {
      "source": "/privacy",
      "destination": "/privacy-terms"
    },
    {
      "source": "/tos",
      "destination": "/privacy-terms"
    },
    {
      "source": "/gdpr",
      "destination": "/privacy-terms#gdpr"
    }
  ]
}
